pipeline {
    agent any
    stages {
        stage("Branch To Build") {
            steps {
                script {
                class Example {
   static void main(String[] args) {
      // Using a simple println statement to print output to the console
      println('Hello World');
   }
   }
}
}
    }
    }
    post { 
        success{ 
               echo 'build is successful'
        }   
        }
        
}
